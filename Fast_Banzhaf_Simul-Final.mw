<Font opaque="true" background="[192,192,192]" encoding="UTF-8">#cliquer ici et taper &quot;entr\303\251e&quot; pour activer.</Font><Font opaque="true" background="[192,192,192]">
</Font>pol := proc (W::list) 
local i, j, n, w::integer, P, Q::array; n := nops(W); 
w := sum(W[i], i = 1 .. n); 
P := array(0 .. w); 
Q := array(0 .. w); 
for j from 0 to w do 
  if j = 0 or j = W[1] then 
    P[j] := 1; 
  else 
    P[j] := 0 ;
  end 
end; #print(boucle1,P);
for i from 2 to n do 
  for j from 0 to w do 
    if W[i] &lt;= j then 
      Q[j] := P[j-W[i]]; 
     else 
       Q[j] := 0 ;
     end;
   end; 
   for j from 0 to w do 
     P[j] := Q[j]+P[j];
    end;#print(boucle||i,P);
end;
RETURN(P);
end:

polk:=proc(W::list,k::integer,P::array) 
local i,j,n,w::integer,Pk::table; 
n:=nops(W); w:=sum(W[i],i=1..n); 
Pk:=array(0..w-W[k]); #print(Pkkkk,Pk);
for j from 0 to min(W[k]-1,w-W[k]) do 
  Pk[j]:=P[j] ;
end; 
for j from W[k] to w-W[k] do 
  Pk[j]:=P[j]-Pk[j-W[k]]; 
end; #print(Pk);
RETURN(Pk); 
end:

nbswing:=proc(W::list,q::integer)  
local i, j,k, n,w,dk::integer, P,Pk::array,Nbs::list ;  
n:=nops(W);  
w:=sum(W[i],i=1..n);  
Nbs:=[seq(0,i=1..n)];  
P:=pol(W); #print(Swing,P);  
for k from 1 to n do #print(Swing,kk,k);
    if (q-W[k])&lt;=min(q-1,w-W[k]) then #print(Swing,k,YES);      
        Pk:=polk(W,k,P);
        Nbs[k]:=sum(Pk[j],j=max(0,(q-W[k]))..min(q-1,w-W[k]));
    end;
end;
RETURN(Nbs);  
end:

incotest:=proc(w,banz)
local i,tes;
tes:=0;
for i from 1 to nops(w)-1 while tes=0 do
  if w[i]&gt;w[i+1] and banz[i]=banz[i+1] then
     tes:=1;
  end;
end;
tes;
end:

simulweights:=proc(nvoters,tweight,quota,taille)
local config,nperm,i,j,res,rep,repp,ress,ens,dconfig;
config:=[seq(1,i=1..nvoters-1),seq(0,i=1..tweight)];
dconfig:=combinat[numbperm](config);
rep:=0;ens:={};
repp:=0;
for i from 1 to taille do
 res:=combinat[randperm](config);#print(aaa,res,nops(res));
 res:=[0,ListTools[SearchAll](1,res),tweight+nvoters];#print(bbb,res,nops(res));
 res:=-sort(-[seq(res[j+1]-res[j]-1,j=1..nvoters)]);#print(res);
 ress:=nbswing(res,quota);
 if incotest(res,ress)=1 then
  ens:={op(ens),[res,1]};
 else  
  ens:={op(ens),[res,0]};
 end;
end:
`+`(seq(i[2],i in ens))/nops(ens)*1.0;
end:</Text-field>
